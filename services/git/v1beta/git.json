{
  "components": {
    "parameters": {
      "AuthenticationId": {
        "description": "Authentication Source identifier.",
        "example": "83b8e9c4-e3d1-4dc4-bb89-9854c8b5383e",
        "explode": false,
        "in": "path",
        "name": "authenticationId",
        "required": true,
        "schema": {
          "$ref": "#/components/schemas/UUID"
        },
        "style": "simple"
      },
      "InstanceId": {
        "description": "Instance identifier.",
        "example": "83b8e9c4-e3d1-4dc4-bb89-9854c8b5383e",
        "explode": false,
        "in": "path",
        "name": "instanceId",
        "required": true,
        "schema": {
          "$ref": "#/components/schemas/UUID"
        },
        "style": "simple"
      },
      "ProjectId": {
        "description": "Project identifier.",
        "example": "83b8e9c4-e3d1-4dc4-bb89-9854c8b5383e",
        "explode": false,
        "in": "path",
        "name": "projectId",
        "required": true,
        "schema": {
          "$ref": "#/components/schemas/UUID"
        },
        "style": "simple"
      }
    },
    "schemas": {
      "Acl": {
        "description": "A list of CIDR network addresses that are allowed to access the instance.",
        "items": {
          "example": "1.2.3.4/1",
          "format": "cidr",
          "type": "string"
        },
        "maxItems": 50,
        "type": "array"
      },
      "Authentication": {
        "description": "Describes an authentication definition associated to a STACKIT Git instance. The provider type will be an openidConnect type.",
        "properties": {
          "auto_discover_url": {
            "description": "The well-known configuration url to use for this authentication definition.",
            "type": "string"
          },
          "client_id": {
            "description": "The IDP client id to use.",
            "type": "string"
          },
          "created_at": {
            "format": "date-time",
            "maxLength": 48,
            "type": "string"
          },
          "icon_url": {
            "description": "The url of the icon to use for this authentication definition.",
            "type": "string"
          },
          "id": {
            "description": "An auto generated unique uuid which identifies the authentication definition in STACKIT Git instances.",
            "format": "uuid",
            "maxLength": 36,
            "type": "string"
          },
          "name": {
            "description": "The name to identify an authentication definition associated with a STACKIT Git instance.",
            "maxLength": 32,
            "type": "string"
          },
          "provider": {
            "description": "The Oauth2 provider to use.",
            "type": "string"
          },
          "scopes": {
            "description": "Scopes defines the OIDC scopes to request.",
            "type": "string"
          },
          "status": {
            "description": "The current status of the authentication definition.",
            "example": "Ready",
            "type": "string"
          }
        },
        "required": [
          "id",
          "created_at",
          "name",
          "client_id",
          "icon_url",
          "auto_discover_url",
          "provider",
          "scopes",
          "status"
        ]
      },
      "AuthenticationList": {
        "description": "A list of authentications belonging to an Instance.",
        "properties": {
          "authentication": {
            "items": {
              "$ref": "#/components/schemas/Authentication"
            },
            "maxItems": 50,
            "type": "array"
          }
        },
        "required": [
          "authentication"
        ]
      },
      "CreateAuthenticationPayload": {
        "description": "Properties to patch on an authentication. All fields are optional.",
        "properties": {
          "auto_discover_url": {
            "description": "The well-known configuration url to use for this authentication definition.",
            "type": "string"
          },
          "client_id": {
            "description": "The IDP client id to use.",
            "type": "string"
          },
          "client_secret": {
            "description": "The IDP client secret to use.",
            "type": "string"
          },
          "icon_url": {
            "description": "The url of the icon to use for this authentication definition.",
            "type": "string"
          },
          "name": {
            "description": "The name to identify an authentication definition associated with a STACKIT Git instance.",
            "maxLength": 32,
            "type": "string"
          },
          "provider": {
            "default": "openidConnect",
            "description": "The Oauth2 provider to use.",
            "type": "string"
          },
          "scopes": {
            "default": "openid profile email",
            "description": "Scopes defines the OIDC scopes to request.",
            "type": "string"
          }
        },
        "required": [
          "name",
          "client_id",
          "client_secret",
          "auto_discover_url"
        ]
      },
      "CreateInstancePayload": {
        "description": "Request a STACKIT Git instance to be created with these properties.",
        "properties": {
          "acl": {
            "$ref": "#/components/schemas/Acl"
          },
          "flavor": {
            "enum": [
              "git-10",
              "git-100"
            ],
            "example": "git-100",
            "type": "string"
          },
          "name": {
            "description": "A user chosen name to distinguish multiple STACKIT Git instances.",
            "maxLength": 32,
            "minLength": 5,
            "pattern": "^[a-z]([a-z0-9\\-]){0,30}[a-z0-9]+$",
            "type": "string"
          }
        },
        "required": [
          "name"
        ]
      },
      "CreateRunnerPayload": {
        "description": "Request a runner to be created supporting the requested list of runtime labels.",
        "properties": {
          "labels": {
            "items": {
              "example": "stackit-docker",
              "type": "string"
            },
            "type": "array"
          }
        },
        "required": [
          "labels"
        ]
      },
      "FeatureToggle": {
        "description": "Feature toggles for the instance.",
        "properties": {
          "default_email_notifications": {
            "description": "Default email notifications.",
            "enum": [
              "enabled",
              "disabled",
              "onmention",
              "andyourown"
            ],
            "nullable": true,
            "type": "string"
          },
          "enable_commit_signatures": {
            "description": "Enable commit signatures.",
            "nullable": true,
            "type": "boolean"
          },
          "enable_local_login": {
            "description": "Enable local login.",
            "nullable": true,
            "type": "boolean"
          }
        },
        "type": "object"
      },
      "Flavor": {
        "description": "Describes a STACKIT Git Flavor.",
        "properties": {
          "availability": {
            "description": "Defines the flavor availability.",
            "enum": [
              "available",
              "unavailable",
              "internal",
              "deprecated"
            ],
            "type": "string"
          },
          "description": {
            "description": "Flavor description.",
            "type": "string"
          },
          "display_name": {
            "description": "The display name that will be shown in the Portal.",
            "type": "string"
          },
          "id": {
            "description": "Flavor id.",
            "maxLength": 36,
            "type": "string"
          },
          "sku": {
            "description": "SAP article number.",
            "type": "string"
          }
        },
        "required": [
          "id",
          "availability",
          "display_name",
          "description",
          "sku"
        ]
      },
      "FlavorsList": {
        "description": "A list of STACKIT Git Flavors.",
        "properties": {
          "flavors": {
            "items": {
              "$ref": "#/components/schemas/Flavor"
            },
            "type": "array"
          }
        },
        "required": [
          "flavors"
        ]
      },
      "GenericErrorResponse": {
        "description": "Generic Error Response.",
        "properties": {
          "details": {
            "items": {
              "type": "string"
            },
            "type": "array"
          },
          "message": {
            "example": "Internal server error.",
            "type": "string"
          }
        },
        "required": [
          "message"
        ],
        "type": "object"
      },
      "Instance": {
        "description": "Describes a STACKIT Git instance.",
        "properties": {
          "acl": {
            "description": "Restricted ACL for instance access.",
            "items": {
              "type": "string"
            },
            "type": "array"
          },
          "consumed_disk": {
            "description": "How many bytes of disk space is consumed. Read Only.",
            "type": "string"
          },
          "consumed_object_storage": {
            "description": "How many bytes of Object Storage is consumed. Read Only.",
            "type": "string"
          },
          "created": {
            "description": "The date and time the creation of the STACKIT Git instance was triggered.",
            "format": "date-time",
            "maxLength": 48,
            "type": "string"
          },
          "feature_toggle": {
            "$ref": "#/components/schemas/FeatureToggle"
          },
          "flavor": {
            "description": "Instance flavor.",
            "type": "string"
          },
          "id": {
            "description": "A auto generated unique id which identifies the STACKIT Git instances.",
            "format": "uuid",
            "maxLength": 36,
            "type": "string"
          },
          "name": {
            "description": "A user chosen name to distinguish multiple STACKIT Git instances.",
            "maxLength": 32,
            "type": "string"
          },
          "state": {
            "description": "The current state of the STACKIT Git instance.",
            "enum": [
              "Creating",
              "WaitingForResources",
              "Updating",
              "Deleting",
              "Ready",
              "Error"
            ],
            "maxLength": 32,
            "type": "string"
          },
          "url": {
            "description": "The URL for reaching the STACKIT Git instance.",
            "maxLength": 2048,
            "type": "string"
          },
          "version": {
            "description": "The current version of STACKIT Git deployed to the instance.",
            "maxLength": 20,
            "type": "string"
          }
        },
        "required": [
          "id",
          "name",
          "url",
          "created",
          "state",
          "version",
          "consumed_disk",
          "consumed_object_storage",
          "flavor",
          "acl",
          "feature_toggle"
        ]
      },
      "InstanceList": {
        "description": "A list of STACKIT Git instances.",
        "properties": {
          "instances": {
            "items": {
              "$ref": "#/components/schemas/Instance"
            },
            "maxItems": 50,
            "type": "array"
          }
        },
        "required": [
          "instances"
        ]
      },
      "InternalServerErrorResponse": {
        "description": "Internal server error.",
        "properties": {
          "error": {
            "example": "Internal server error.",
            "type": "string"
          }
        },
        "type": "object"
      },
      "PatchAuthenticationPayload": {
        "description": "Properties to patch on an authentication. All fields are optional.",
        "properties": {
          "auto_discover_url": {
            "description": "The well-known configuration url to use for this authentication definition.",
            "type": "string"
          },
          "client_id": {
            "description": "The IDP client id to use.",
            "type": "string"
          },
          "client_secret": {
            "description": "The IDP client secret to use.",
            "type": "string"
          },
          "icon_url": {
            "description": "The url of the icon to use for this authentication definition.",
            "type": "string"
          },
          "provider": {
            "description": "The Oauth2 provider to use.",
            "type": "string"
          },
          "scopes": {
            "description": "Scopes defines the OIDC scopes to request.",
            "type": "string"
          }
        }
      },
      "PatchInstancePayload": {
        "description": "Properties to patch on an instance. All fields are optional.",
        "properties": {
          "acl": {
            "allOf": [
              {
                "$ref": "#/components/schemas/Acl"
              }
            ],
            "nullable": true
          },
          "feature_toggle": {
            "$ref": "#/components/schemas/FeatureToggle"
          }
        },
        "type": "object"
      },
      "PatchOperation": {
        "description": "Request a STACKIT Git instance to be patch with these properties.",
        "properties": {
          "op": {
            "description": "The patch operation to perform.",
            "enum": [
              "add",
              "remove"
            ],
            "type": "string"
          },
          "path": {
            "description": "An RFC6901 JSON Pointer to the target location.",
            "example": "/acl",
            "type": "string"
          },
          "value": {
            "description": "The value to be used for 'add' and 'remove' operations.",
            "example": "A new value",
            "type": "string"
          }
        },
        "required": [
          "op",
          "path"
        ],
        "type": "object"
      },
      "Runner": {
        "description": "Describes a runner associated to a STACKIT Git instance.",
        "properties": {
          "created_at": {
            "format": "date-time",
            "maxLength": 48,
            "type": "string"
          },
          "id": {
            "format": "uuid",
            "maxLength": 36,
            "type": "string"
          },
          "labels": {
            "items": {
              "example": "stackit-docker",
              "type": "string"
            },
            "type": "array"
          },
          "status": {
            "description": "The current status of the runner.",
            "example": "Ready",
            "type": "string"
          }
        },
        "required": [
          "id",
          "created_at",
          "labels",
          "status"
        ]
      },
      "RunnerRuntime": {
        "description": "Describes a STACKIT Git Runner runtime.",
        "properties": {
          "availability": {
            "description": "Indicates the availability of the runner label",
            "example": "available",
            "type": "string"
          },
          "description": {
            "description": "Human-friendly description of the runtime and it's capabilities.",
            "example": "A runner capable of building Docker images and runner Docker containers.",
            "type": "string"
          },
          "display_name": {
            "description": "Human-friendly name of the runtime.",
            "type": "string"
          },
          "id": {
            "description": "Runtime identifier.",
            "example": "stackit-docker",
            "type": "string"
          },
          "label": {
            "description": "Runtime label.",
            "example": "stackit-docker",
            "type": "string"
          }
        },
        "required": [
          "id",
          "label",
          "display_name",
          "description",
          "availability",
          "status"
        ]
      },
      "RunnerRuntimeList": {
        "description": "A list of STACKIT Git RunnerLabels.",
        "properties": {
          "items": {
            "items": {
              "$ref": "#/components/schemas/RunnerRuntime"
            },
            "type": "array"
          }
        },
        "required": [
          "items"
        ]
      },
      "UUID": {
        "description": "Universally Unique Identifier (UUID).",
        "example": "d61a8564-c8dd-4ffb-bc15-143e7d0c85ed",
        "format": "uuid",
        "maxLength": 36,
        "minLength": 36,
        "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
        "type": "string"
      }
    }
  },
  "info": {
    "contact": {
      "email": "git@stackit.cloud",
      "name": "STACKIT Git",
      "url": "https://docs.stackit.cloud/stackit/en/git-261161358.html"
    },
    "description": "STACKIT Git management API.",
    "title": "STACKIT Git API",
    "version": "1beta.0.4"
  },
  "openapi": "3.0.3",
  "paths": {
    "/v1beta/projects/{projectId}/flavors": {
      "get": {
        "description": "Provides detailed information about possible Git Flavors.\n",
        "operationId": "ListFlavors",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/FlavorsList"
                }
              }
            },
            "description": "Git flavors."
          },
          "400": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericErrorResponse"
                }
              }
            },
            "description": "Bad Request."
          },
          "401": {
            "description": "Unauthorized."
          },
          "404": {
            "description": "Not found."
          },
          "500": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericErrorResponse"
                }
              }
            },
            "description": "Internal server error."
          }
        },
        "summary": "Returns the details for the given STACKIT Git flavors.",
        "x-stackit-authorization": {
          "actions": [
            "git.instance.create"
          ],
          "resource-id": "projectId",
          "resource-id-type": "dynamic",
          "resource-type": "project"
        }
      },
      "parameters": [
        {
          "$ref": "#/components/parameters/ProjectId"
        }
      ]
    },
    "/v1beta/projects/{projectId}/instances": {
      "get": {
        "description": "Lists all STACKIT Git instances within a project.\n",
        "operationId": "GetInstances",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/InstanceList"
                }
              }
            },
            "description": "Successful response with all instances."
          },
          "400": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericErrorResponse"
                }
              }
            },
            "description": "Bad Request."
          },
          "401": {
            "description": "Unauthorized."
          },
          "500": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericErrorResponse"
                }
              }
            },
            "description": "Internal server error."
          }
        },
        "summary": "List Instances.",
        "x-stackit-authorization": {
          "actions": [
            "git.instance.list"
          ],
          "resource-id": "projectId",
          "resource-id-type": "dynamic",
          "resource-type": "project"
        }
      },
      "parameters": [
        {
          "$ref": "#/components/parameters/ProjectId"
        }
      ],
      "post": {
        "description": "Creates a new STACKIT Git instance as a project resource.\n",
        "operationId": "CreateInstance",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateInstancePayload"
              }
            }
          },
          "description": "Instance configuration options.",
          "required": true
        },
        "responses": {
          "202": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Instance"
                }
              }
            },
            "description": "Instance has been created."
          },
          "400": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericErrorResponse"
                }
              }
            },
            "description": "Bad Request."
          },
          "401": {
            "description": "Unauthorized."
          },
          "409": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericErrorResponse"
                }
              }
            },
            "description": "Conflict."
          },
          "500": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericErrorResponse"
                }
              }
            },
            "description": "Internal server error."
          }
        },
        "summary": "Create an Instance.",
        "x-stackit-authorization": {
          "actions": [
            "git.instance.create"
          ],
          "resource-id": "projectId",
          "resource-id-type": "dynamic",
          "resource-type": "project"
        }
      }
    },
    "/v1beta/projects/{projectId}/instances/{instanceId}": {
      "delete": {
        "description": "Deletes a STACKIT Git instance and destroys all associated data.\n",
        "operationId": "DeleteInstance",
        "responses": {
          "204": {
            "description": "Accepted."
          },
          "400": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericErrorResponse"
                }
              }
            },
            "description": "Bad Request."
          },
          "401": {
            "description": "Unauthorized."
          },
          "404": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericErrorResponse"
                }
              }
            },
            "description": "Not found."
          },
          "409": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericErrorResponse"
                }
              }
            },
            "description": "Conflict."
          },
          "500": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericErrorResponse"
                }
              }
            },
            "description": "Internal server error."
          }
        },
        "summary": "Delete Instance.",
        "x-stackit-authorization": {
          "actions": [
            "git.instance.delete"
          ],
          "resource-id": "projectId",
          "resource-id-type": "dynamic",
          "resource-type": "project"
        }
      },
      "get": {
        "description": "Retrieves information about a STACKIT Git instance.",
        "operationId": "GetInstance",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Instance"
                }
              }
            },
            "description": "OK"
          },
          "400": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericErrorResponse"
                }
              }
            },
            "description": "Bad Request."
          },
          "401": {
            "description": "Unauthorized."
          },
          "404": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericErrorResponse"
                }
              }
            },
            "description": "Not found."
          },
          "500": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericErrorResponse"
                }
              }
            },
            "description": "Internal server error."
          }
        },
        "summary": "Get Instance information.",
        "x-stackit-authorization": {
          "actions": [
            "git.instance.get"
          ],
          "resource-id": "projectId",
          "resource-id-type": "dynamic",
          "resource-type": "project"
        }
      },
      "parameters": [
        {
          "$ref": "#/components/parameters/ProjectId"
        },
        {
          "$ref": "#/components/parameters/InstanceId"
        }
      ],
      "patch": {
        "description": "Patches the Instance.\n",
        "operationId": "PatchInstance",
        "requestBody": {
          "content": {
            "application/json": {
              "example": {
                "acl": [
                  "1.1.1.1/1",
                  "1.2.3.4/5"
                ],
                "feature_toggle": {
                  "default_email_notifications": "enabled",
                  "enable_local_login": true
                }
              },
              "schema": {
                "$ref": "#/components/schemas/PatchInstancePayload"
              }
            },
            "application/json-patch+json": {
              "schema": {
                "example": [
                  {
                    "op": "remove",
                    "path": "/acl",
                    "value": "1.1.1.1/1"
                  }
                ],
                "items": {
                  "$ref": "#/components/schemas/PatchOperation"
                },
                "type": "array"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Instance"
                }
              }
            },
            "description": "Instance updated."
          },
          "202": {
            "description": "Instance was updated."
          },
          "400": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericErrorResponse"
                }
              }
            },
            "description": "Bad Request."
          },
          "401": {
            "description": "Unauthorized."
          },
          "404": {
            "description": "Not found."
          },
